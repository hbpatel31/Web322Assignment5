{
    "version": 3,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "PGDATABASE": "@pg-database",
      "PGUSER": "@pg-user",
      "PGPASSWORD": "@pg-password",
      "PGHOST": "@pg-host"
    }
  }
  
